//LLenar y mostrar un arreglo
Proceso Arreglo_clase1
	
	Definir ar_num Como Entero;
	Dimension ar_num[5];
	
	//llenado manual del arreglo
	ar_num[0] <- 1;
	ar_num[1] <- 2;
	ar_num[2] <- 3;
	ar_num[3] <- 4;
	ar_num[4] <- 5;
	
	//mostrar los elementos del arreglo
	Escribir ar_num[2];
	Escribir ar_num[4];
	
FinProceso
